
@model IEnumerable<VisitorManager.Models.Visitor>


<table id="userTable" class="table datatable-show-all">
    <thead>
        <tr>
            <th>Name</th>
            <th>Mobile</th>
            <th>Visit To</th>
            <th>Purpose</th>
            <th>Veh. No.</th>
            <th>Time In</th>
            <th>Time Out</th>
        </tr>
    </thead>
    <tbody>
        @if (Model == null) { }
        else
        {
            foreach (var item in Model)
            {
        <tr>
            <td>@item.VisitorName</td>
            <td>@item.Mobile</td>
            <td>@item.WhomToVisit</td>
            <td>@item.Purpose</td>
            <td>@item.VhicleNumber</td>
            <td>@item.TimeIn</td>
            <td>@item.TimeOut</td>
            
        </tr>
            }
        }

    </tbody>
</table>


