<body class="login-container">
    <!-- Main navbar -->
    <div class="navbar navbar-inverse">
        <div class="navbar-header">
            <a class="navbar-brand" href="index.html"><img src="assets/images/logo_light.png" alt=""></a>
            <ul class="nav navbar-nav pull-right visible-xs-block">
                <li><a data-toggle="collapse" data-target="#navbar-mobile"><i class="icon-tree5"></i></a></li>
            </ul>
        </div>
        <div class="navbar-collapse collapse" id="navbar-mobile">
            <ul class="nav navbar-nav navbar-right">
                <li>
                    <a href="#">
                        <i class="icon-display4"></i> <span class="visible-xs-inline-block position-right"> Go to website</span>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="icon-user-tie"></i> <span class="visible-xs-inline-block position-right"> Contact admin</span>
                    </a>
                </li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown">
                        <i class="icon-cog3"></i>
                        <span class="visible-xs-inline-block position-right"> Options</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <!-- /main navbar -->
    <!-- Page container -->
    <div class="page-container">
        <!-- Page content -->
        <div class="page-content">
            <!-- Main content -->
            <div class="content-wrapper">
                <!-- Content area -->
                <div class="content">
                    <!-- Registration form -->
                    <form>
                        <div class="row">
                            <div class="col-lg-6 col-lg-offset-3">
                                <div class="panel registration-form">
                                    <div class="panel-body">
                                        <div class="text-center">
                                            <div class="icon-object border-success text-success"><i class="icon-plus3"></i></div>
                                            <h5 class="content-group-lg">Create account <small class="display-block">All fields are required</small></h5>
                                        </div>
                                        <div class="form-group has-feedback">
                                            <select id="ddl_Package" class="form-control input-lg"></select>
                                            <label id="lbl_Package" style="color:red;"></label>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <input type="text" class="form-control" id="txt_AdminId" value="@ViewBag.adminAppId" readonly />
                                                    @*<div class="form-control-feedback">
                    <i class="icon-user-check text-muted"></i>
                </div>*@
                                                    <label id="lbl_AdminId" style="color:red;"></label>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <input type="text" id="txt_Name" class="form-control" placeholder="Name">
                                                    <div class="form-control-feedback">
                                                        <i class="icon-user-check text-muted"></i>
                                                    </div>
                                                    <label id="lbl_Name" style="color:red;"></label>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <input type="text" id="txt_Email" class="form-control" placeholder="Email" onchange="mainPage.CheckAlreadyExistEmail()">
                                                    <div class="form-control-feedback">
                                                        <i class="icon-user-check text-muted"></i>
                                                    </div>
                                                    <label id="lbl_Email" style="color:red;"></label>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <input type="text" id="txt_Mobile" class="form-control" placeholder="Mobile">
                                                    <div class="form-control-feedback">
                                                        <i class="icon-user-check text-muted"></i>
                                                    </div>
                                                    <label id="lbl_Mobile" style="color:red;"></label>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <select id="ddl_Country" class="form-control input-lg" onchange="mainPage.GetState()"></select>

                                                    <label id="lbl_Country" style="color:red;"></label>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <select id="ddl_State" class="form-control input-lg"></select>

                                                    <label id="lbl_State" style="color:red;"></label>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <input type="text" id="txt_City" class="form-control" placeholder="City">
                                                    <div class="form-control-feedback">
                                                        <i class="icon-user-check text-muted"></i>
                                                    </div>
                                                    <label id="lbl_City" style="color:red;"></label>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <input type="text" id="txt_ZipCode" class="form-control" placeholder="Zip Code">
                                                    <div class="form-control-feedback">
                                                        <i class="icon-user-check text-muted"></i>
                                                    </div>
                                                    <label id="lbl_ZipCode" style="color:red;"></label>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <input type="text" id="txt_Username" class="form-control" placeholder="User Name" onchange="mainPage.CheckAlreadyExistUserName()">
                                                    <div class="form-control-feedback">
                                                        <i class="icon-user-check text-muted"></i>
                                                    </div>
                                                    <label id="lbl_Username" style="color:red;"></label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">

                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <input type="password" id="txt_Password" class="form-control" placeholder="Create password">
                                                    <div class="form-control-feedback">
                                                        <i class="icon-user-lock text-muted"></i>
                                                    </div>
                                                    <label id="lbl_Password" style="color:red;"></label>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <input type="password" id="txt_Confirm" class="form-control" placeholder="Repeat password">
                                                    <div class="form-control-feedback">
                                                        <i class="icon-user-lock text-muted"></i>
                                                    </div>
                                                    <label id="lbl_Confirm" style="color:red;"></label>
                                                </div>
                                            </div>
                                        </div>
                                        @*<div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <input type="email" class="form-control" placeholder="Your email">
                                                    <div class="form-control-feedback">
                                                        <i class="icon-mention text-muted"></i>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group has-feedback">
                                                    <input type="email" class="form-control" placeholder="Repeat email">
                                                    <div class="form-control-feedback">
                                                        <i class="icon-mention text-muted"></i>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>*@
                                        <div class="form-group">
                                           
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" id="ckb_Terms" class="styled">
                                                    Accept <a href="#">terms of service</a>
                                                </label>
                                            </div>
                                            <div class="col-md-12">
                                                <label id="lbl_Accept" style="color:red;"></label>
                                            </div>
                                        </div>
                                        <div class="text-right">
                                            <button type="button" onclick="mainPage.RedirectToLogin(event)" class="btn btn-link"><i class="icon-arrow-left13 position-left"></i> Back to login form</button>
                                            <button type="submit" class="btn bg-teal-400 btn-labeled btn-labeled-right ml-10" onclick="mainPage.InsertNewUser()"><b><i class="icon-plus3"></i></b> Create account</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <!-- /registration form -->
                    <!-- Footer -->
                    <div class="footer text-muted text-center">
                        &copy; 2015. <a href="#">Limitless Web App Kit</a> by <a href="http://themeforest.net/user/Kopyov" target="_blank">Eugene Kopyov</a>
                    </div>
                    <!-- /footer -->
                </div>
                <!-- /content area -->
            </div>
            <!-- /main content -->
        </div>
        <!-- /page content -->
    </div>
    <!-- /page container -->
</body>
