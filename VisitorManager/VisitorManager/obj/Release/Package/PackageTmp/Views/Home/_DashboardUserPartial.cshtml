                                    </a>
                                    <ul class="dropdown-menu dropdown-menu-right">
                                        <li><a href="" onclick="gateKeeper.OpenUserDeletePopUp(@item.Id)"><i class="icon-file-pdf"></i> Delete</a></li>
                                        <li><a href="Home/UpdateVisitorManager?id=@item.Id"><i class="icon-file-excel"></i> Edit</a></li>

                                    </ul>
                                </li>
                            </ul>*@
                    @*</td>*@
                </tr>
            }
