
@model IEnumerable<VisitorManager.Models.AdminDetails>


<table id="userTable" class="table datatable-show-all">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Mobile</th>
            <th>Package</th>
            <th>Status</th>
            @*<th class="text-center">Action</th>*@
        </tr>
    </thead>
    <tbody>
        @if (Model == null) { }
        else
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Email</td>
                    <td>@item.Mobile</td>
                    <td>@item.PackageName</td>
                    <td>@item.Status</td>
